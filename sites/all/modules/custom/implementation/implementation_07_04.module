    {
        foreach ($media_ids as $mId)
        {
            $media_company_name = db_query("SELECT field_company_organization_value FROM field_data_field_company_organization WHERE entity_id =" . $mId)->fetchField();
            if ($media_company_name != '')
            {
                $media_company_names[$mId] = $media_company_name;
            }
        }
    }

